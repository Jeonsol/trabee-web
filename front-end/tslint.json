{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "jsx-no-lambda": false,
    "disable-next-line": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "no-debugger": false,
    "no-console": false,
    "no-unused-variable": [
      false
    ],
    "interface-name": false,
  },
  "linterOptions": {
    "exclude": [
      "node_modules/**/*.ts",
      "src/utils/moment.js"
    ]
  }
}